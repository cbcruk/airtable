{
  "name": "@cbcruk/airtable",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "version": "0.0.8",
  "scripts": {
    "build": "tsup --format esm,cjs --dts --legacy-output",
    "release": "yarn build && yarn publish --access=public"
  },
  "devDependencies": {
    "@types/node": "^17.0.30",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbcruk/airtable.git"
  }
}
